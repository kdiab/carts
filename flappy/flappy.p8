function dead_update()
		if score > best then
				best = score
		end
		if game_state == "dead" then
				if btnp(4) then
						game_init()
						game_state = "game"
				end
		end
		if pos.y > 126 then
				pos.y = 126
		end
end

function dead_draw()
		cls(12)
		map()
		draw_pipes()
		spr(1,pos.x,pos.y)
		print("controls: z to jump!",28,73,0)
		print("press z to start!",33,63)
		print("score: "..score.." | best: "..best,30,53)
end
