function game_update()
		if pos.y >= 120 then
				game_state = "dead"
		end
		update_pipes()
		if game_state == "game" then
			if btnp(4) then
					sfx(0) 
					dy = -4
					flying = true
					last_position = pos.y
					player_sprite = 0x10
			end
			dy += gravity
			pos.y += dy
			if pos.y >= last_position then
					flying = false
					player_sprite = 0
			end
			if pos.y <= -10 then
				pos.y = -10
				dy = 2
			end
		end
end

function game_draw()
		map()
		draw_pipes()
		spr(player_sprite, pos.x, pos.y)
		print(score, 65, 23)
end

function game_init()
		flying = false
		pos = {x=63, y=63}
		dy = 0
		last_position = 0
		pipes = {}
		score = 0
end

function collision(a, b)
		p_diff = 40
		return not 
		((a.x > b.x+12
		or a.y > b.z+6
		or a.x+4 < b.x
	 or a.y < -8)
		and
		(a.x > b.x+12
		or a.y > 128
		or a.x+4 < b.x
		or a.y < b.z+p_diff-8))
end
