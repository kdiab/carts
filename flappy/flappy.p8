function update_pipes()
		del_pipes()
		add_pipe()
		for p in all(pipes) do
				p.x -= 1
				p.w -= 1
		end
		for p in all(pipes) do
				if collision(pos,p) then
						game_state = "dead"
				end
		end
end

function draw_pipes()
		height_diff = 40
		for p in all(pipes) do
			spr(p_l, p.x, p.y)
			spr(p_r, p.x+8, p.y)
			spr(pd_l, p.w, p.z)
			spr(pd_r, p.w+8, p.z)
			
			spr(pu_l, p.w, p.z+height_diff)
			spr(pu_r, p.w+8, p.z+height_diff)
			spr(p_l, p.x, p.y+height_diff+16)
			spr(p_r, p.x+8, p.y+height_diff+16)
		end
		
		for p in all(pipes) do
			p_up = p.y
			p_down = p.y
			while p_up > -8 do
					spr(p_l, p.x, p_up)
					spr(p_r, p.x+8, p_up)
					p_up -= 8
			end
			while p_down < 120 do
					spr(p_l, p.x, p_down+height_diff+16)
					spr(p_r, p.x+8, p_down+height_diff+16)
					p_down += 8
			end
		end
end

function add_pipe()
		height = flr(rnd(8))*8
		if #pipes < 2 then
				if #pipes == 0 then
						add(pipes,{x=120, y=height, w=120, z=height+8})
				elseif pipes[1].x < 63 then
						score+=1
						add(pipes,{x=120, y=height, w=120, z=height+8})
				end
		end				
end

function del_pipes()
		for p in all(pipes) do
				if p.w+8 < 0 then
					del(pipes,p)
				end
		end
end
