function _init()
		game_state = "menu"
		flying = false
		pos = {x=63, y=63}
		dy = 0
		gravity = 0.45
		last_position = 0
		score = 0
		best = 0
		
		p_l = 2
		p_r = 3
		pd_l = 6
		pd_r = 7
		pu_l = 4
		pu_r = 5
		player_sprite = 0
		
		pipes = {{x=120, y=0, w=120, z=8}}
end

function _update()
		if game_state == "menu" then
				menu_update()
		elseif game_state == "game" then
				--game_dbg()
				game_update()
		elseif game_state == "dead" then
				dead_update()
		end
end

function _draw()
				if game_state == "menu" then
				menu_draw()
		elseif game_state == "game" then
				game_draw()
				--game_dbg_draw()
		elseif game_state == "dead" then
				dead_draw()
		end
end


