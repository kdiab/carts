function game_dbg()
		if btn(⬅️) then pos.x -= 1 end
		if btn(➡️) then pos.x += 1 end
		if btn(⬆️) then pos.y -= 1 end
		if btn(⬇️) then pos.y += 1 end
		pipes[1] = {x=62,y=32,w=62,z=40}
end

function game_dbg_draw()
		print(collision(pos,pipes[1]))
end
