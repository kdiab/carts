-- game update & draw

function _update()
		if debug then
				update_dbg()
		end
		if game_state == "menu" then
				update_menu()
		elseif game_state == "game" then
				update_player()
--				update_planes()
--				update_rockets()
		elseif game_state == "dead" then
		end
end

function _draw()
		cls()
		if game_state == "menu" then
				draw_menu()
		elseif game_state == "game" then
				map(
					map_coords.cx,
					map_coords.cy,
					map_coords.sx,
					map_coords.sy
				)
				draw_planes()
				draw_player()
				draw_rockets()
		elseif game_state == "dead" then
		end
		if debug then
				draw_dbg()
		end
end
-->8
-- planes

function init_planes()
		planes = {}
		spawn_timer = 30
end

function update_planes()
		if spawn_timer < 0 then
				spawn_plane()
				spawn_timer = 30+rnd(60)
		end
		spawn_timer -= 1
		for p in all(planes) do
				p.x += p.speed
				if p.x > 68 then
						del(planes,p)
				end
		end
end

function draw_planes()
		for p in all(planes) do
				spr(p.sp,
				p.x,
				p.y,
				2,2)
		end
end

function spawn_plane()
		local x = 0
		local y = 40 + flr(rnd(50))
		local sp = {8,10,40}
		local p = {
				sp=sp[flr(rnd(3)+1)],
				x=x,
				y=y,
				speed=1
				}
		add(planes,p)
end
-->8
-- player

function init_player()
		player = {x=63, y=120}
end

function update_player()
		if btn(⬅️) then
				if player.x > -6 then
						player.x -= 1
				end
		end
		if btn(➡️) then
				if player.x < 118 then
						player.x += 1
				end
		end
end

function draw_player()
		spr(42,player.x,player.y,2,1)
end
-->8
-- rockets

function init_rockets()
		rockets = {}
		max_rockets = 3
		rocket_speed = 2
end

function update_rockets()
		if btnp(4) and #rockets < max_rockets then
				spawn_rocket()
		end
		for r in all(rockets) do
				r.y -= r.s
				if r.y < 0 then
						del(rockets, r)
				end
		end
end

function spawn_rocket()
		local r = {
				x=player.x+4,
				y=player.y - 4,
				s=rocket_speed
		}
		add(rockets, r)
end

function draw_rockets()
		for r in all(rockets) do
				spr(17,r.x,r.y)
		end
end
