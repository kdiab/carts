-- planes

function init_planes()
		planes = {}
		spawn_timer = 30
end

function update_planes()
		if spawn_timer < 0 then
				spawn_plane()
				spawn_timer = 30+rnd(60)
		end
		spawn_timer -= 1
		for p in all(planes) do
				p.x += p.speed
				if p.x > 68 then
						lives -= 1
						del(planes,p)
						if lives == 0 then
								if score > best_score then
										best_score = score
								end
								game_state = "dead"
						end
				end
		end
end

function draw_planes()
		for p in all(planes) do
				spr(p.sp,
				p.x,
				p.y,
				2,2)
		end
end

function spawn_plane()
		local x = -8
		local y = 40 + flr(rnd(50))
		local sp = {8,10,40}
		local p = {
				sp=sp[flr(rnd(3)+1)],
				x=x,
				y=y,
				speed=1
				}
		add(planes,p)
end
