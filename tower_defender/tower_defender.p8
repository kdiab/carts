-- init	

function _init()
		debug = false
		score = 0
		lives = 3
		best_score = 0
		palt(0, false)
		palt(15,true)
		game_state="menu"
		map_coords = {
				cx=0,
				cy=0,
				sx=0,
				sy=0
		}
		init_ui()
		init_menu()
		init_planes()
		init_player()
		init_rockets()
		init_particles()
		if debug then
				init_dbg()
		end
end

