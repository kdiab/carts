-- game update & draw

function _update()
		if debug then
				update_dbg()
		end
		if game_state == "menu" then
				update_menu()
		elseif game_state == "game" then
				update_ui()
				update_player()
				update_planes()
				update_rockets()
				update_particles()
		elseif game_state == "dead" then
				update_ui()
				update_dead()
		end
end

function _draw()
		cls()
		if game_state == "menu" then
				draw_menu()
		elseif game_state == "game" then
				map(
					map_coords.cx,
					map_coords.cy,
					map_coords.sx,
					map_coords.sy
				)
				print("score: "..score,1,18,0)
				draw_planes()
				draw_player()
				draw_rockets()
				draw_particles()
				draw_ui()
		elseif game_state == "dead" then
				draw_dead()
				draw_ui()
		end
		if debug then
				draw_dbg()
		end
end
