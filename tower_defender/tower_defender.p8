function spawn_plane_dbg()
		local x = 32
		local y = 40 + flr(rnd(50))
		local sp = {8,10,40}
		local p = {
				sp=sp[flr(rnd(3)+1)],
				x=x,
				y=y,
				speed=0
				}
		add(planes,p)
end

function spawn_rocket_dbg()
		local r = {
				x=67,
				y=63,
				s=0
		}
		add(rockets, r)
end
