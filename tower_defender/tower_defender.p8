-- rockets

function init_rockets()
		rockets = {}
		max_rockets = 3
		rocket_speed = 0.07
end

function update_rockets()
		if (btnp(4) or btnp(‚ùé)) and #rockets < max_rockets then
--				sfx(0)
				spawn_rocket()
		end
		for r in all(rockets) do
				r.y -= r.s
				r.s += rocket_speed
				if r.y < 0 then
						del(rockets, r)
				end
				for p in all(planes) do
						if collided(r,p) then
								del(planes,p)
								del(rockets, r) 
								score += 1
						end
				end
		end
end

function spawn_rocket()
		local r = {
				x=player.x+4,
				y=player.y - 6,
				s=0.2
		}
		add(rockets, r)
end

function draw_rockets()
		for r in all(rockets) do
				spr(17,r.x,r.y)
		end
end
