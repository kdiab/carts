-- init	

function _init()
		debug = true
		palt(0, false)
		palt(15,true)
		game_state="menu"
		map_coords = {
				cx=0,
				cy=0,
				sx=0,
				sy=0
		}
		init_planes()
		init_player()
		init_rockets()
		if debug then
				init_dbg()
		end
end

