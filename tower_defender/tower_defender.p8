function init_ui()
		local h = 0
		local w = 1
		local rh = 8
		hearts = {{x=w,y=h,sp=33},
				{x=w + 8,y=h,sp=33},
				{x=w + 16,y=h,sp=33}
				}
		rocket_stock = {
				{x=w,y=rh,sp=17},
				{x=w + 8,y=rh,sp=17},
				{x=w + 16,y=rh,sp=17}
				}
end

function update_ui()
				if lives == 3 then
						for h in all(hearts) do
								h.sp = 33
						end
				elseif lives == 2 then
						hearts[3].sp = 49
				elseif lives == 1 then
						hearts[2].sp = 49
				elseif lives == 0 then
						hearts[1].sp = 49
				end
end

function draw_ui()
		for h in all(hearts) do
				spr(h.sp,h.x,h.y)
		end
		for r=1, max_rockets - #rockets do
				spr(rocket_stock[r].sp,
				rocket_stock[r].x,
				rocket_stock[r].y)
		end
end
